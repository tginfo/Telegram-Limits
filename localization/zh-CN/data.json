{
  "accounts": {
    "name": "个人账号相关",
    "items": {
      "username": {
        "name": "用户名",
        "hint": "（例：@cameraman）",
        "text": "5 ~ 32 个字符"
      },
      "bio": {
        "name": "个人简介",
        "hint": "",
        "text": "最多 70 个字符"
      },
      "name_length": {
        "name": "名字长度",
        "hint": "",
        "text": "1 ~ 64 个字符"
      },
      "last_name_length": {
        "name": "姓氏长度",
        "hint": "",
        "text": "0 ~ 64 个字符"
      },
      "self_destruction": {
        "name": "账户自动销毁时限",
        "hint": "（长时间无登录活动）",
        "text": "1、3、6 或 12 个月 (默认 6 个月)"
      },
      "spam_ban": {
        "name": "恶意灌水封禁时长",
        "hint": "",
        "text": "3 天或以上"
      },
      "channels_and_chats_number": {
        "name": "频道和超级群组数量",
        "hint": "你可以加入的",
        "text": "最多 500（包括您创建的）"
      },
      "geochats_number": {
        "name": "当地群组数量",
        "hint": "你可以加入的",
        "text": "最多 5 个（包括你创建的）"
      },
      "gifs_number": {
        "name": "保存在 GIF 库的 GIF 数量",
        "hint": "",
        "text": "最多 200"
      },
      "video_length": {
        "name": "视频头像时长",
        "hint": "",
        "text": "最长 10 秒"
      },
      "groups_and_channels_creation": {
        "name": "创建频道和群组",
        "hint": "",
        "text": "一天最多 50 个"
      },
      "accounts_number": {
        "name": "同时登录的账号数",
        "hint": "（官方客户端）",
        "text": "最多 3 个"
      }
    }
  },
  "chats": {
    "name": "群组相关",
    "items": {
      "participants_number": {
        "name": "群成员",
        "hint": "(可以提升限制)",
        "text": "最多 20 万；广播群组中无限制"
      },
      "admins_number": {
        "name": "管理员数量",
        "hint": "群组中的",
        "text": "最多 50 个"
      },
      "bots_amount": {
        "name": "机器人数量",
        "hint": "群组中的",
        "text": "最多 20 个"
      },
      "pinned_number": {
        "name": "客户端主页面的置顶群聊数",
        "hint": "",
        "text": "最多 5 个群聊 / 频道 + 5 个私密聊天"
      },
      "group_name": {
        "name": "群组名称和简介",
        "hint": "",
        "text": "最多 255 个字符"
      },
      "editing": {
        "name": "编辑消息",
        "hint": "",
        "text": "发送后 48 小时内；收藏夹中不受限制，拥有置顶权限的群管理员不受限制"
      },
      "media_self_destruction": {
        "name": "媒体自毁时限",
        "hint": "",
        "text": "1 ~ 60 秒"
      },
      "recent_actions": {
        "name": "群组的最近操作",
        "hint": "（新订阅者、消息更改等）",
        "text": "最近两天"
      },
      "mentions_amount": {
        "name": "可以 @ 的成员个数",
        "hint": "单条消息中",
        "text": "最多 50 个，否则不会发送被 @ 通知"
      },
      "visible_messages": {
        "name": "可见的消息数",
        "hint": "在群组中",
        "text": "最新的 100 万条消息"
      },
      "pinned_messages_number": {
        "name": "可置顶的消息数",
        "hint": "",
        "text": "不限"
      },
      "participants_to_see_stats": {
        "name": "使“统计数据”可用的频道订阅者 / 群聊成员数",
        "hint": "",
        "text": "至少 500"
      },
      "username_reservation": {
        "name": "更改用户名生效时长",
        "hint": "（更改多长时间后对所有人可见）",
        "text": "15 ~ 30 分钟"
      },
      "participants_for_group_stickers": {
        "name": "群组贴纸",
        "hint": "",
        "text": "拥有大于 100 名成员的群组"
      },
      "chat_role_length": {
        "name": "群管理头衔长度",
        "hint": "",
        "text": "最多 16 个字符"
      },
      "chat_title_length": {
        "name": "频道 / 群聊标题长度",
        "hint": "",
        "text": "最多 128 个字符"
      },
      "broadcast_group_access": {
        "name": "超级群组可转为广播群组",
        "hint": "",
        "text": "拥有超过 199,000 位成员的群组"
      },
      "read_receipts_availability": {
        "name": "“哪些成员已读”数据可见性",
        "hint": "",
        "text": "少于 100 人的群聊可用；只有对应消息的发送者可见"
      },
      "read_receipts_lifetime": {
        "name": "“哪些成员已读”数据可见性",
        "hint": "",
        "text": "消息发送后 7 天内"
      }
    }
  },
  "voice_chats": {
    "name": "语音 & 视频聊天相关",
    "items": {
      "voice_chat_participants": {
        "name": "语音聊天听众数",
        "hint": "",
        "text": "不限数量"
      },
      "video_chat_broadcast": {
        "name": "广播视频",
        "hint": "（通过摄像头 / 屏幕共享）",
        "text": "最多 30 人（同时）"
      },
      "video_chat_viewers": {
        "name": "视频聊天的观众数",
        "hint": "",
        "text": "官方声称为“不限”；实际测试中，观众达到约 2,000 时，直播流可能会变得不稳定。"
      },
      "voice_chat_schedule": {
        "name": "可提前预定的时长",
        "hint": "",
        "text": "最多提前 7 天"
      }
    }
  },
  "channels": {
    "name": "频道相关",
