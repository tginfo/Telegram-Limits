{
  "accounts": {
    "name": "个人账号相关",
    "items": {
      "username": {
        "name": "用户名",
        "hint": "（例：@cameraman）",
        "text": "5 ~ 32 个字符"
      },
      "bio": {
        "name": "个人简介",
        "hint": "",
        "text": "最多 70 个字符"
      },
      "name_length": {
        "name": "名字长度",
        "hint": "",
        "text": "1 ~ 64 个字符"
      },
      "last_name_length": {
        "name": "姓氏长度",
        "hint": "",
        "text": "0 ~ 64 个字符"
      },
      "self_destruction": {
        "name": "账户自动销毁时限",
        "hint": "（长时间无登录活动）",
        "text": "1、3、6 或 12 个月 (默认 6 个月)"
      },
      "spam_ban": {
        "name": "恶意灌水封禁时长",
        "hint": "",
        "text": "from 1 day up to forever"
      },
      "channels_and_chats_number": {
        "name": "频道和超级群组数量",
        "hint": "你可以加入的",
        "text": "最多 500（包括您创建的）"
      },
      "geochats_number": {
        "name": "当地群组数量",
        "hint": "你可以加入的",
        "text": "最多 5 个（包括你创建的）"
      },
      "gifs_number": {
        "name": "保存在 GIF 库的 GIF 数量",
        "hint": "",
        "text": "最多 200"
      },
      "video_length": {
        "name": "视频头像时长",
        "hint": "",
        "text": "最长 10 秒"
      },
      "groups_and_channels_creation": {
        "name": "创建频道和群组",
        "hint": "",
        "text": "一天最多 50 个"
      },
      "accounts_number": {
        "name": "同时登录的账号数",
        "hint": "（官方客户端）",
        "text": "最多 3 个"
      }
    }
  },
  "chats": {
    "name": "群组相关",
    "items": {
      "participants_number": {
        "name": "群成员",
        "hint": "(可以提升限制)",
        "text": "最多 20 万；广播群组中无限制"
      },
      "admins_number": {
        "name": "管理员数量",
        "hint": "群组中的",
        "text": "最多 50 个"
      },
      "bots_amount": {
        "name": "机器人数量",
        "hint": "群组中的",
        "text": "最多 20 个"
      },
      "pinned_number": {
        "name": "客户端主页面的置顶群聊数",
        "hint": "",
        "text": "最多 5 个群聊 / 频道 + 5 个私密聊天"
      },
      "group_name": {
        "name": "群组名称和简介",
        "hint": "",
        "text": "最多 255 个字符"
      },
      "editing": {
        "name": "编辑消息",
        "hint": "",
        "text": "发送后 48 小时内；收藏夹中不受限制，拥有置顶权限的群管理员不受限制"
      },
      "media_self_destruction": {
        "name": "媒体自毁时限",
        "hint": "",
        "text": "1 ~ 60 秒"
      },
      "recent_actions": {
        "name": "群组的最近操作",
        "hint": "（新订阅者、消息更改等）",
        "text": "最近两天"
      },
      "mentions_amount": {
        "name": "可以 @ 的成员个数",
        "hint": "单条消息中",
        "text": "最多 50 个，否则不会发送被 @ 通知"
      },
      "visible_messages": {
        "name": "可见的消息数",
        "hint": "在群组中",
        "text": "最新的 100 万条消息"
      },
      "pinned_messages_number": {
        "name": "可置顶的消息数",
        "hint": "",
        "text": "不限"
      },
      "participants_to_see_stats": {
        "name": "使“统计数据”可用的频道订阅者 / 群聊成员数",
        "hint": "",
        "text": "至少 500"
      },
      "username_reservation": {
        "name": "更改 username 后生效时长",
        "hint": "（更改多长时间后对所有人可见）",
        "text": "15 ~ 30 分钟"
      },
      "participants_for_group_stickers": {
        "name": "群组贴纸",
        "hint": "",
        "text": "拥有大于 100 名成员的群组"
      },
      "chat_role_length": {
        "name": "群管理头衔长度",
        "hint": "",
        "text": "最多 16 个字符"
      },
      "chat_title_length": {
        "name": "频道 / 群聊标题长度",
        "hint": "",
        "text": "最多 128 个字符"
      },
      "broadcast_group_access": {
        "name": "超级群组可转为广播群组",
        "hint": "",
        "text": "拥有超过 199,000 位成员的群组"
      },
      "read_receipts_availability": {
        "name": "“哪些成员已读”数据可见性",
        "hint": "",
        "text": "少于 100 人的群聊可用；只有对应消息的发送者可见"
      },
      "read_receipts_lifetime": {
        "name": "“哪些成员已读”数据可见性",
        "hint": "",
        "text": "消息发送后 7 天内"
      }
    }
  },
  "voice_chats": {
    "name": "语音 & 视频聊天相关",
    "items": {
      "voice_chat_participants": {
        "name": "语音聊天听众数",
        "hint": "",
        "text": "不限数量"
      },
      "video_chat_broadcast": {
        "name": "广播视频",
        "hint": "（通过摄像头 / 屏幕共享）",
        "text": "最多 30 人（同时）"
      },
      "video_chat_viewers": {
        "name": "视频聊天的观众数",
        "hint": "",
        "text": "官方声称为“不限”；实际测试中，观众达到约 2,000 时，直播流可能会变得不稳定。"
      },
      "voice_chat_schedule": {
        "name": "可提前预定的时长",
        "hint": "",
        "text": "最多提前 7 天"
      }
    }
  },
  "channels": {
    "name": "频道相关",
    "items": {
      "public_usernames": {
        "name": "可拥有的公开频道 / 群组个数",
        "hint": "单个用户",
        "text": "最多 10 个（包括群组）"
      },
      "username_length": {
        "name": "用户名长度",
        "hint": "",
        "text": "5 ~ 32 个字符"
      },
      "channel_name": {
        "name": "频道名称和简介",
        "hint": "",
        "text": "最多 255 个字符"
      },
      "channel_admins": {
        "name": "频道管理员数",
        "hint": "(包括机器人；您可以通过将机器人设置为管理员将机器人添加到您的频道)",
        "text": "最多 50 个"
      },
      "editing": {
        "name": "推文可编辑次数",
        "hint": "",
        "text": "不限（如果频道有讨论组，发送超过 2 周的推文不会在讨论组中更新）"
      },
      "subscribers": {
        "name": "频道订阅者数",
        "hint": "",
        "text": "不限"
      },
      "visible_participants": {
        "name": "在列表中可见的订阅者",
        "hint": "（仅管理员可见）",
        "text": "200 人"
      },
      "channel_deletion": {
        "name": "删除频道",
        "hint": "",
        "text": "拥有超过 1K 订阅者的频道只能通过 Telegram 支持删除频道"
      },
      "private_channel_access": {
        "name": "不加入的情况下查看私密频道",
        "hint": "（订阅者 > 200 的频道）",
        "text": "每天不超过 5 分钟"
      },
      "invintations": {
        "name": "邀请用户加入频道",
        "hint": "",
        "text": "最多 200 人"
      },
      "followers_to_see_stats": {
        "name": "“统计数据”可见的订阅者数阈值",
        "hint": "",
        "text": "至少 50 位订阅者"
      },
      "username_reservation": {
        "name": "更改 username 生效时长",
        "hint": "（更改多长时间后对所有人可见）",
        "text": "15 ~ 30 分钟"
      }
    }
  },
  "messages": {
    "name": "消息相关",
    "items": {
      "single_message_length": {
        "name": "单条消息长度",
        "hint": "",
        "text": "最多 4,096 个字符"
      },
      "media_descriptions": {
        "name": "媒体的描述",
        "hint": "",
        "text": "最多 4096 个字符"
      },
      "upload_size": {
        "name": "单文件大小限制",
        "hint": "",
        "text": "up to 2.0 GB"
      },
      "file_name_length": {
        "name": "文件名长度限制",
        "hint": "",
        "text": "最多 60 个字符，多出的将被裁切"
      },
      "video_message_duration": {
        "name": "视频消息的时长限制",
        "hint": "",
        "text": "最长 1 分钟"
      },
      "album_size": {
        "name": "单条消息中的图片和视频",
        "hint": "（或专辑）",
        "text": "最多 10 个"
      },
      "delayed_messages": {
        "name": "计划信息数量",
        "hint": "",
        "text": "最多 100 个"
      },
      "delaying_messages": {
        "name": "可计划的日程范围",
        "hint": "",
        "text": "最长 365 天"
      }
    }
  },
  "folders": {
    "name": "聊天文件夹相关",
    "items": {
      "folder_amount": {
        "name": "文件夹数量",
        "hint": "",
        "text": "最多 10 个文件夹"
      },
      "pinned_chats_in_folder": {
        "name": "单个文件夹中可置顶的聊天数",
        "hint": "",
        "text": "最多 100 个"
      }
    }
  },
  "people_nearby": {
    "name": "附近的人",
    "items": {
      "range": {
        "name": "可见范围",
        "hint": "",
        "text": "约 12 公里以内"
      },
      "amount_of_people": {
        "name": "可见用户数量",
        "hint": "",
        "text": "最多 100 人"
      },
      "geochats_number": {
        "name": "可见的当地群组数量",
        "hint": "",
        "text": "最多 10 个附近的群组"
      }
    }
  },
  "bots": {
    "name": "机器人相关",
    "items": {
      "bots_amount": {
        "name": "单账户可拥有的机器人数量",
        "hint": "（由 @BotFather 创建的）",
        "text": "最多 20 个"
      },
      "username length": {
        "name": "用户名长度",
        "hint": "机器人的",
        "text": "5 ~ 32 个字符"
      },
      "info_length": {
        "name": "机器人关于信息（What can this bot do?）长度",
        "hint": "(/setabouttext)",
        "text": "最多 120 个字符"
      },
      "description_length": {
        "name": "机器人简介长度",
        "hint": "",
        "text": "最多 512 个字符"
      },
      "upload_limit_botapi": {
        "name": "上传文件大小限制",
        "hint": "不使用本地 Bot API 后端",
        "text": "最大 50 MB"
      },
      "upload_limit_botapi_local": {
        "name": "上传文件大小限制",
        "hint": "使用本地 Bot API 后端",
        "text": "最大 2000 MB"
      },
      "download_limit_botapi": {
        "name": "下载文件大小限制",
        "hint": "不使用本地 Bot API 后端",
        "text": "最大 20 MB"
      },
      "download_limit_botapi_local": {
        "name": "下载文件大小限制",
        "hint": "使用本地 Bot API 后端",
        "text": "最大 2000 MB"
      },
      "keys_in_keyboard_number": {
        "name": "键盘按钮数",
        "hint": "",
        "text": "最多 100 个实体"
      },
      "formatting_data": {
        "name": "Message markup data",
        "hint": "e.g. inline buttons",
        "text": "最大 10 KB"
      },
      "formatting_entities": {
        "name": "格式化的实体",
        "hint": "（加粗、斜体...）",
        "text": "最多 100 个"
      },
      "messaging_frequency_broadcast": {
        "name": "消息广播速率",
        "hint": "仅适用于主动发送通知到不活跃的聊天",
        "text": "对话中每秒最多 30 条消息"
      },
      "messaging_frequency": {
        "name": "消息发送速率",
        "hint": "任意对话中",
        "text": "每秒最多约 1 条消息（短时间连发的情况可能不包括在内）"
      },
      "messaging_frequency_groups": {
        "name": "消息发送速率",
        "hint": "在群组种",
        "text": "同对话中每秒最多 20 条消息"
      },
      "bot_commands_count": {
        "name": "指令数量",
        "hint": "(Bot Father)",
        "text": "0 ~ 100 个命令"
      },
      "bot_command_name_length": {
        "name": "指令长度",
        "hint": "(Bot Father)",
        "text": "1 ~ 32 个字符"
      },
      "bot_command_description_length": {
        "name": "指令描述长度",
        "hint": "",
        "text": "最多 256 个字符；在通过 Bot API 设置时不可为空"
      },
      "start_payload_size": {
        "name": "/start 命令的元数据长度",
        "hint": "（深度链接中）",
        "text": "最多 64 字节"
      },
      "bot_alert_length": {
        "name": "弹窗提示文本（响应回调时）",
        "hint": "",
        "text": "最多 200 个字符"
      },
      "inline_max_items": {
        "name": "内联查询结果数",
        "hint": "",
        "text": "每页最多 50 条"
      }
    }
  },
  "search": {
    "name": "搜索相关",
    "items": {
      "global_search": {
        "name": "全局搜索结果",
        "hint": "",
        "text": ""
      },
      "global_search_min_query": {
        "name": "搜索内容字数",
        "hint": "（全局搜索）",
        "text": "至少 4 个字符"
      }
    }
  },
  "stickers": {
    "name": "贴纸相关",
    "items": {
      "added_packs": {
        "name": "可添加的贴纸包个数",
        "hint": "",
        "text": "最多 200 个"
      },
      "favorite_stickers": {
        "name": "收藏贴纸数",
        "hint": "",
        "text": "最多 5 个"
      },
      "pack_size": {
        "name": "单个贴纸包中的贴纸数",
        "hint": "",
        "text": "静态 120 张 / 动态 50 张"
      },
      "sticker_size": {
        "name": "贴纸比例",
        "hint": "",
        "text": "单侧 512px，另一侧 ≤ 512px；动态贴纸必须为 512×512"
      },
      "animation_duration": {
        "name": "动态贴纸持续时长",
        "hint": "",
        "text": "最长 3 秒"
      },
      "file_size": {
        "name": "贴纸文件大小",
        "hint": "",
        "text": "静态最大 512 KB，动画最大 64 KB"
      },
      "fps": {
        "name": "帧速率 (FPS)",
        "hint": "",
        "text": "30 或 60"
      },
      "stickerpack_name_length": {
        "name": "贴纸包名称",
        "hint": "",
        "text": "最多 64 个字符"
      },
      "stickerpack_url_length": {
        "name": "/addstickers 贴纸包链接",
        "hint": "",
        "text": "最多 62 个字符"
      }
    }
  },
  "telegraph": {
    "name": "Telegraph 相关",
    "items": {
      "accounts_number": {
        "name": "账号数",
        "hint": "@telegraph 机器人中",
        "text": "最多 5 个"
      },
      "text_size": {
        "name": "文章大小",
        "hint": "",
        "text": "最大 64 KiB"
      }
    }
  },
  "miscellaneous": {
    "name": "其它",
    "items": {
      "properties": {
        "name": "视频头像属性",
        "hint": "",
        "text": "800x800px，最大 2 MB"
      },
      "photo_resolution": {
        "name": "Photo resolution",
        "hint": "when sent with compression",
        "text": "each side should be less or equal to 1280px, else it will be downscaled so the biggest side will be 1280px"
      },
      "username_resolve_limit": {
        "name": "用户名解析限制",
        "hint": "(适用于机器人和用户)",
        "text": "每日最多 200 个"
      }
    }
  }
}
