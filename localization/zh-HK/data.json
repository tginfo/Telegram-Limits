{
  "accounts": {
    "name": "個人帳戶",
    "items": {
      "username": {
        "name": "使用者名稱",
        "hint": "(例如： @hkersaddoil)",
        "text": "5 至 32 個英數字符（符號只限底線）"
      },
      "bio": {
        "name": "個人簡介",
        "hint": "",
        "text": "最多 70 個字"
      },
      "name_length": {
        "name": "使用者本名",
        "hint": "",
        "text": "1 至 64 個字符"
      },
      "last_name_length": {
        "name": "使用者姓氏",
        "hint": "",
        "text": "【選填】0 至 64 個字符"
      },
      "self_destruction": {
        "name": "「閒置帳戶」自動刪除計時器",
        "hint": "（即使用者在指定時間內沒有上線或登入使用）",
        "text": "1、3、6 或 12 個月（預設為 6 個月）"
      },
      "spam_ban": {
        "name": "濫發訊息封鎖時間",
        "hint": "",
        "text": "一般為 3 日（再次干犯者會自動疊加封鎖日數）"
      },
      "channels_and_chats_number": {
        "name": "可以加入的頻道及大型群組數目",
        "hint": "（即 channel 以及連同 Bot 在內，超過 49 人的群組）",
        "text": "最多 500 個（包括你管理、創立嘅 channel 及大型群組）"
      },
      "gifs_number": {
        "name": "可以儲存的 GIF 圖數目",
        "hint": "",
        "text": "最多 200 張"
      },
      "video_length": {
        "name": "動態大頭貼秒數",
        "hint": "",
        "text": "最多 10 秒"
      },
      "groups_and_channels_creation": {
        "name": "可以建立群組及頻道嘅數目",
        "hint": "",
        "text": "每日最多 50 個"
      },
      "accounts_number": {
        "name": "可以同時登入的帳戶數目",
        "hint": "（以 Telegram 發佈的應用程式為準）",
        "text": "一般情況下，最多 3 個"
      }
    }
  },
  "chats": {
    "name": "聊天室與群組",
    "items": {
      "participants_number": {
        "name": "對話成員",
        "hint": "（特定情況下可以由 Telegram 酌情提升）",
        "text": "大型群組最多 20 萬人，但廣播群組則無人數上限"
      },
      "admins_number": {
        "name": "管理員數量",
        "hint": "（只限制於群組對話）",
        "text": "最多 50 人"
      },
      "bots_amount": {
        "name": "Bot 的數量",
        "hint": "（只限制於群組對話）",
        "text": "最多 20 個"
      },
      "pinned_number": {
        "name": "可以在首頁置頂的對話數量",
        "hint": "",
        "text": "最多 5 個對話及頻道，以及 5 個加密對話"
      },
      "group_name": {
        "name": "群組名稱及簡介",
        "hint": "",
        "text": "最多 255 個字"
      },
      "editing": {
        "name": "訊息編輯時限",
        "hint": "",
        "text": "一般為 48 小時（被置頂的訊息或擁有置頂權限嘅管理員，則不受此限）"
      },
      "media_self_destruction": {
        "name": "限時圖片及影片",
        "hint": "",
        "text": "1 至 60 秒"
      },
      "recent_actions": {
        "name": "最近操作",
        "hint": "（包括加入或退出嘅成員、訊息編輯、權限變動等）",
        "text": "過去 48 小時"
      },
      "mentions_amount": {
        "name": "一個訊息內可以提及的使用者數目",
        "hint": "（不論對話有否被靜音，被提及的使用者都會收到通知）",
        "text": "最多 50 人（再多就全部人都唔會收到通知）"
      },
      "visible_messages": {
        "name": "可以讀取嘅群組訊息數量",
        "hint": "（不論時間遠近，只計算訊息數量）",
        "text": "最新嘅 100 萬條訊息"
      },
      "pinned_messages_number": {
        "name": "可以置頂嘅訊息數量",
        "hint": "",
        "text": "無數量上限"
      },
      "participants_to_see_stats": {
        "name": "查看群組統計資訊嘅門檻",
        "hint": "",
        "text": "最少 500 個成員"
      },
      "username_reservation": {
        "name": "使用者名稱保留時長",
        "hint": "（改名之後可以保留幾耐，先會釋放俾其他人登記使用）",
        "text": "大約 15 至 30 分鐘"
      },
      "participants_for_group_stickers": {
        "name": "設定群組貼圖包嘅門檻",
        "hint": "",
        "text": "最少 100 個成員"
      },
      "chat_role_length": {
        "name": "管理員身份名稱",
        "hint": "",
        "text": "最多 16 個字"
      },
      "chat_title_length": {
        "name": "Chat title length",
        "hint": "",
        "text": "最多 128 個字"
      },
      "broadcast_group_access": {
        "name": "將大型群組轉換為廣播群組的門檻",
        "hint": "",
        "text": "最少 199,000 個成員"
      }
    }
  },
  "voice_chats": {
    "name": "語音通話",
    "items": {
      "voice_chat_participants": {
        "name": "收聽成員數量",
        "hint": "",
        "text": "無數量上限"
      },
      "video_chat_broadcast": {
        "name": "同時直播影像的成員數目",
        "hint": "（包括相機鏡頭、螢幕分享，每個影像計算為一個分享數目）",
        "text": "最多 30 條影像同時直播"
      },
      "video_chat_viewers": {
        "name": "直播影像觀看人數",
        "hint": "",
        "text": "最多 1000 人同時收睇"
      },
      "voice_chat_schedule": {
        "name": "預定訊息",
        "hint": "",
        "text": "最多提早 7 日"
      }
    }
  },
  "channels": {
    "name": "頻道",
    "items": {
      "public_usernames": {
        "name": "可以開立的頻道數目",
        "hint": "（以單一使用者帳戶計算）",
        "text": "最多 10 個（包括群組對話）"
      },
      "username_length": {
        "name": "頻道的「@使用者名稱」長度",
        "hint": "",
        "text": "5 至 32 個英數字符（符號只限底線）"
      },
      "channel_name": {
        "name": "頻道名稱和簡介",
        "hint": "",
        "text": "最多 255 個字"
      },
      "channel_admins": {
        "name": "頻道管理員數目",
        "hint": "（包括 Bot 的數量）",
        "text": "最多 50 個"
      },
      "editing": {
        "name": "訊息編輯時限",
        "hint": "",
        "text": "無限制（但如果頻道有連結到一個群組對話，則超過兩星期的訊息在編輯後，不會被同步更新）"
      },
      "subscribers": {
        "name": "頻道訂閱人數",
        "hint": "",
        "text": "無數量上限"
      },
      "visible_participants": {
        "name": "頻道訂閱成員清單",
        "hint": "（只限俾管理員睇）",
        "text": "最多只可以睇到 200 人"
      },
      "channel_deletion": {
        "name": "刪除頻道的門檻",
        "hint": "",
        "text": "多於 1000 個訂閱人數的頻道，只能夠由 Telegram 刪除"
      },
      "private_channel_access": {
        "name": "在不加入非公開頻道的情況下閱讀內容",
        "hint": "（門檻為頻道需要有最少 200 個訂閱人數）",
        "text": "每日只可以睇 5 分鐘"
      },
      "invintations": {
        "name": "邀請數目",
        "hint": "",
        "text": "最多 200 人"
      },
      "followers_to_see_stats": {
        "name": "查看頻道統計資訊嘅門檻",
        "hint": "",
        "text": "最少 50 個訂閱人數"
      },
      "username_reservation": {
        "name": "頻道「@使用者名稱」嘅保留時長",
        "hint": "（改名之後可以保留幾耐，先會釋放俾其他人登記使用）",
        "text": "大約 15 至 30 分鐘"
      }
    }
  },
